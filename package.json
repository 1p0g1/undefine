{
  "name": "undefine",
  "version": "1.0.0",
  "description": "A Word Guessing Game",
  "main": "dist-server/index.js",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "[ \"$CI\" = \"true\" ] || exit 0",
    "build": "npm run build:types && npm run build:server && npm run build:client",
    "build:minimal": "npm run build:types && npm run build:client",
    "build:types": "cd packages/shared-types && tsc",
    "build:server": "tsc -p src/tsconfig.server.json",
    "build:client": "cd client && npm run build",
    "build:client-only": "cd client && npm run build",
    "start": "node dist-server/index.js",
    "start:client-only": "cd client && npm run preview",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/index.ts",
    "dev:client": "cd client && npm run dev",
    "lint": "eslint .",
    "fix:imports": "node fix-imports.js",
    "fix:tests": "node fix-test-files.js"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.24",
    "axios": "^1.8.4",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "get-port": "^7.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-pool": "^3.8.0",
    "prom-client": "^14.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.13",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.1",
    "@types/morgan": "^1.9.4",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.11.11",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "wait-on": "^8.0.3"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.5.3"
  }
}
